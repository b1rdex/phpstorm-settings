              <quotes left="'" right="'" leftEscaped="''" rightEscaped="''" />
            </quotation>
          </record-format>
        </data>
      </csv-format>
      <csv-format name="Excel CSV" id="87d65d9b-e527-413d-a85a-343311e630a4">
